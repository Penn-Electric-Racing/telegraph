    "core-js": "^3.7.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.0",
    "signals": "^1.0.0",
    "telegraph": "file:../js",
    "uplot": "^1.1.2",
    "uuid": "^3.3.3",
    "vue": "^2.6.10",
    "vue-grid-layout": "^2.3.7",
    "vue-js-toggle-button": "^1.3.3",
    "vue-json-pretty": "^1.6.3",
    "vue-resize-split-pane": "^0.1.5",
    "vuex": "^3.5.1",
    "webpack": "^4.43.0"
