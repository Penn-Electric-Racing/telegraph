      <Value Regex="tireTemperature"/>
    </Group>
  </Device>
  <Device Name="Charger" Identifier="charger" Orientation="Horizontal" StartVisible="true" Hideable="false">
    <Group Name="Faults">
      <Value Regex="charger\.faults\." FaultState="True"/>
    </Group>
    <Group Name="Data" Orientation="Vertical">
      <Value Regex="charger\.maxPower"/>
      <Value Regex="charger\.outletVoltage"/>
      <Value Regex="charger\.outletCurrent"/>
