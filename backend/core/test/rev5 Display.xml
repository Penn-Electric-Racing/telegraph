    <Group Name="Connected" Orientation="Vertical">
      <Value Regex="sdl\.connections"/>
    </Group>
    <Group Name="Faults" Orientation="Vertical">
      <Value Regex="sdl\.faults\." FaultState="True"/>
    </Group>
    <Group Name="Param" Orientation="Vertical">
      <Value Regex="sdl\.param\."/>
    </Group>
    <Group Name="Xbees" Orientation="Vertical">
      <Value Regex="sdl\.xb1\."/>
      <Value Regex="sdl\.xb2\."/>
