      <Value Regex="ams\.stack\.thermistors"/>
    </Group>
    <Group Name="Parameters" Hideable="true" StartVisible="false" >
      <Value Regex="ams\.param"/>
    </Group>
    <Group Name="Connected" Orientation="Vertical">
      <Value Regex="ams\.connections"/>
      <Value Regex="slavesConnected"/>
    </Group>
  </Device>
  <Device Name="Telemetry" Identifier="sdl" Orientation="Horizontal" StartVisible="true" Hideable="false">
